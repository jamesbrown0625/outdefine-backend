{
  "name": "outdefine-backend-project",
  "version": "1.0.0",
  "description": "Outdefine backend project",
  "main": "serverless.ts",
  "scripts": {
    "build": "tsc",
    "db:sync": "cross-env NODE_ENV=development node --loader ts-node/esm src/build/db-sync.ts",
    "db:sync:prod": "cross-env NODE_ENV=production node --loader ts-node/esm src/build/db-sync.ts",
    "start": "sls offline start --allowCache",
    "deploy-dev": "cross-env NODE_ENV=development sls deploy",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy",
    "watch": "nodemon --exec \"sls offline start\"",
    "lint": "eslint -c .eslintrc --fix ./src/**/*.ts",
    "test": "jest --coverage --silent --reporter=json-summary .> coverage/coverage-report.txt",
    "test-dev": "jest --testPathPattern=tests/controller/assessment"
  },
  "engines": {
    "node": ">=16.15.0"
  },
  "dependencies": {
